<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ookie.dietapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/drown_those_pounds_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/drown_those_pounds_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Login"></activity>
        <activity android:name=".view.About" />
        <activity android:name=".view.SplashPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.MainActivity" />
        <activity android:name=".view.DietDetail" />
    </application>

</manifest>